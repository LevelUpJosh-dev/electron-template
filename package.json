{
  "name": "@redvanjosh/electron-template",
  "version": "0.0.1",
  "author": "Red Van Josh",
  "license": "MIT",
  "description": "A template for creating Electron apps with extensive documentation and examples as well as native TailwindCSS support.",
  "type": "commonjs",
  "main": "main.js",
  "scripts": {
    "electron:start": "electron .",
    "prettier:check": "prettier --check ./Frontend/**/*.{js,html,css} ./Handlers/**/*.js ./Helpers/**/*.js ./main.js",
    "prettier:fix": "prettier --write ./Frontend/**/*.{js,html,css} ./Handlers/**/*.js ./Helpers/**/*.js ./main.js",
    "tailwind:build": "npx tailwindcss -i ./tailwind.css -o ./Frontend/Css/main.css",
    "tailwind:watch": "npx tailwindcss -i ./tailwind.css -o ./Frontend/Css/main.css --watch"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "electron",
    "development",
    "training",
    "tutorial",
    "documentation",
    "salesforce",
    "javascript"
  ],
  "dependencies": {
    "flowbite": "^1.5.3",
    "fuse-fetch": "^1.0.5",
    "prettier": "^2.7.1"
  },
  "devDependencies": {
    "electron": "^21.1.1",
    "tailwindcss": "^3.2.1"
  }
}
